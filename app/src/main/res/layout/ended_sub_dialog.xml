<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="20dp"
    android:background="#CE272525">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="303dp"
        android:layout_height="166dp"
        android:text="Your subscription has ended. Please renew it to keep active as a collaborator. "
        android:textAlignment="center"
        android:textColor="#FFEB3B"
        android:fontFamily="@font/magra"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.128" />

    <Button
        android:id="@+id/okbuttonSubEnded"
        android:layout_width="206dp"
        android:layout_height="70dp"
        android:layout_marginBottom="376dp"
        android:backgroundTint="#FAF03E"
        android:text="@string/ok"
        android:fontFamily="@font/magra"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:layout_constraintVertical_bias="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>