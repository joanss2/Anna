<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.anna">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Anna">
        <activity
            android:name=".MenuPrincipal.MenuMainActivity"
            android:exported="true"
            android:label="@string/title_activity_menu_main"
            android:theme="@style/Theme.Anna.NoActionBar" />
        <activity
            android:name=".MenuPrincipal.Faqs.FragmentFaqs"
            android:exported="true"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".MenuPrincipal.Profile.FragmentProfile"
            android:exported="true"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".MenuPrincipal.Home.FragmentHome"
            android:exported="true"
            tools:ignore="Instantiatable" />
        <activity android:name=".MenuPrincipal.Faqs.Faqs" />
        <activity android:name=".MenuPrincipal.Faqs.ContactUs" />
        <activity android:name=".MenuPrincipal.Home.Discounts.DiscountsActivity" />
        <activity android:name=".MenuPrincipal.Routes.RoutesActivity" />
        <activity android:name=".Ajustes" />
        <activity
            android:name=".Register.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="${MAPS_API_KEY}" />
    </application>

</manifest>